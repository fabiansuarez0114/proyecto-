generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id         Int      @id @default(autoincrement())
  title      String
  assignedTo String?
  date       DateTime
  completed  Boolean  @default(false)
  createdAt  DateTime @default(now())
}
